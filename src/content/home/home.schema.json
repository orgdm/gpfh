{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Home",
  "description": "GPFH home page content",
  "type": "object",
  "properties": {
    "IntroCarousel": {
      "type": "object",
      "properties": {
        "imgs": {
          "type": "array"
        }
      }
    },
    "WhoWeAre": {
      "type": "object",
      "properties": {
        "img": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string"
            },
            "alt": {
              "type": "string"
            },
            "url": {
              "type": "string"
            }
          }
        },
        "text": {
          "type": "string"
        }
      }
    },
    "ServiceCarousel": {
      "type": "object",
      "properties": {
        "services": {
          "type": "array"
        },
        "text": {
          "type": "string"
        }
      }
    },
    "OurStandards": {
      "type": "object",
      "properties": {
        "text": {
          "type": "string"
        },
        "img": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string"
            },
            "alt": {
              "type": "string"
            },
            "url": {
              "type": "string"
            }
          }
        }
      }
    },
    "Environment": {
      "type": "object",
      "properties": {
        "items": {
          "type": "array"
        }
      }
    },
    "Contact": {
      "type": "object",
      "properties": {
        "text": {
          "type": "string"
        }
      }
    }
  },
  "readOnly": true,
  "required": [
    "IntroCarousel",
    "WhoWeAre",
    "ServiceCarousel",
    "OurStandards",
    "Environment"
  ]
}